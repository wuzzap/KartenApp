        <Button Content="Close" HorizontalAlignment="Left" Margin="244,425,0,-40" VerticalAlignment="Top" Width="75" Click="Button_Click_Close"/>
        <Label Content="Kategorie" HorizontalAlignment="Left" Margin="55,335,0,0" VerticalAlignment="Top" Width="64"/>
        <TextBlock Name="msg" HorizontalAlignment="Left" Margin="56,479,0,-114" TextWrapping="Wrap" VerticalAlignment="Top" Height="42" Width="264"/>

    </Grid>
</Window>
